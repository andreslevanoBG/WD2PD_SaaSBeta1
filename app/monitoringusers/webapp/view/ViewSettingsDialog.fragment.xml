<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<ViewSettingsDialog id="viewSettingsDialog" confirm=".onConfirmViewSettingsDialog" cancel="handleCancel" sortDescending="true"
		resetFilters=".handleResetFilters">
		<sortItems>
			<ViewSettingsItem text="{i18n>masterSort1}" key="status_code" selected="false"/>
			<ViewSettingsItem text="{i18n>masterSort2}" key="timestamp_start" selected="true"/>
			<ViewSettingsItem text="{i18n>masterSort3}" key="numberOfItems" selected="false"/>
			<ViewSettingsItem text="{i18n>masterSort4}" key="numberOfSeconds" selected="false"/>
		</sortItems>
		<filterItems>
			<ViewSettingsCustomItem id="idCustomFilterItem" text="{i18n>runDates}" key="timestamp_start">
				<customControl>
					<l:VerticalLayout class="sapUiContentPadding" width="100%">
						<Label text="{i18n>From}"/>
						<DateTimePicker id="DTP1" value="" valueFormat="yyyy-MM-dd-HH-mm-ss" displayFormat="long" change="changeDates"
							class="sapUiSmallMarginBottom"/>
						<Label text="{i18n>To}"/>
						<DateTimePicker id="DTP2" value="" valueFormat="yyyy-MM-dd-HH-mm-ss" displayFormat="long" change="changeDates"
							class="sapUiSmallMarginBottom"/>
					</l:VerticalLayout>
				</customControl>
			</ViewSettingsCustomItem>
			<ViewSettingsFilterItem text="Status" key="status_code">
				<items>
					<ViewSettingsItem text="{i18n>Success}" key="S"/>
					<ViewSettingsItem text="{i18n>Error}" key="E"/>
					<ViewSettingsItem text="{i18n>Warning}" key="W"/>
					<ViewSettingsItem text="{i18n>Running}" key="R"/>
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsCustomItem id="idCustomFilterItem2" text="{i18n>NumUser}" key="numberOfItems">
				<customControl>
					<RangeSlider showAdvancedTooltip="true" range="0,0" min="0" max="200" width="100%" showHandleTooltip="false" inputsAsTooltips="true"
						class="sapUiLargeMarginBottom" change="changeSlider"/>
				</customControl>
			</ViewSettingsCustomItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>