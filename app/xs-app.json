{
    "welcomeFile": "index.html",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/integration/(.*)$",
            "authenticationType": "xsuaa",
            "destination": "srv"
        }
        ,
        {
            "source": "^/v2/integration/(.*)$",
            "authenticationType": "xsuaa",
            "destination": "srv"
        }
        ,
        {
            "source": "^/extend/(.*)",
            "target": "$1",
            "authenticationType": "none",
            "destination": "srv"
        }
        ,
        {
            "source": "^/(.*)$",
            "authenticationType": "xsuaa",
            "localDir": "resources"
        }
    ]
}