{
	"_version": "1.8.0",
	"sap.app": {
		"id": "shapein.ConfiguringOrganizations",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		},
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "Organization",
					"action": "Configuration",
					"icon": "sap-icon://settings",
					"title": "Organizations",
					"info": "",
					"subTitle": "Settings"
				}
			}
		},
		"dataSources": {
			"mainService": {
				"uri": "/ShapeinIntegration/v2/Integration/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/Integration/metadata.xml"
				}
			},
			"json_model_data_source": {
				"uri": "model/mock_al01.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "shapein.ConfiguringOrganizations.view.Configuration",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "shapein.ConfiguringOrganizations.i18n.i18n"
				}
			},
			"mock_json_model": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "json_model_data_source"
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "mainService",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "shapein.ConfiguringOrganizations.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "TargetConfiguration",
					"pattern": "RouteConfiguration",
					"target": [
						"TargetConfiguration"
					]
				}
			],
			"targets": {
				"TargetConfiguration": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Configuration"
				}
			}
		}
	},
	"sap.platform.cf": {
		"oAuthScopes": [
			"$XSAPPNAME.organizations"
		]
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}