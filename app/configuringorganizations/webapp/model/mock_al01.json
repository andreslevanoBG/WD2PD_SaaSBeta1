{
	"web_services": {
		"ws_get_locations": {
			"mappings": {
				"address_data": {
					"filters": {
						"filter": [{
							"usage_public": "1",
							"type_data_primary": "1",
							"type_reference": "BUSINESS",
							"_secuence": "1"
						}, {
							"usage_public": "1",
							"type_data_primary": "0",
							"type_reference": "BUSINESS",
							"_secuence": "2"
						}]
					},
					"country_logic": {
						"country": [{
							"address1": "{ADDRESS_LINE_1} - Number: {ADDRESS_LINE_3}",
							"address2": "{ADDRESS_LINE_2}",
							"address3": "{ADDRESS_LINE_4}",
							"_id": "Spain"
						}]
					}
				},
				"zip_code": {
					"default_values": {
						"country": [{
							"_id": "ES",
							"__text": "28001"
						}, {
							"_id": "DE",
							"__text": "28002"
						}]
					}
				}
			}
		},
		"ws_get_organizations": {
			"request_filter": {
				"include_inactive_orgs": "False"
			}
		},
		"ws_get_workers": {
			"mappings": {
				"email": {
					"filters": {
						"filter": [{
							"usage_public": "1",
							"type_data_primary": "1",
							"type_reference": "WORK",
							"_secuence": "1"
						}, {
							"usage_public": "1",
							"type_data_primary": "1",
							"type_reference": "HOME",
							"_secuence": "2"
						}]
					}
				}
			}
		}
	},
	"transformations": {
		"organization_code": {
			"transformation": [{
				"characters": "/",
				"separator": ";",
				"replace_by": "_",
				"_secuence": "1"
			}, {
				"characters": "Ã‘;~",
				"separator": ";",
				"replace_by": "-",
				"_secuence": "2"
			}]
		}
	}
}