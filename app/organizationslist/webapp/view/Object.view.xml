<mvc:View controllerName="shapein.OrganizationsList.controller.Object" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.m.semantic" xmlns:footerbar="sap.ushell.ui.footerbar">
	<semantic:DetailPage id="page" headerPinnable="false" toggleHeaderOnTitleClick="false" busy="{objectView>/busy}"
		busyIndicatorDelay="{objectView>/delay}">
		<!--<semantic:titleHeading>-->
		<!--	<Title visible="false" text="{ parts:[{path:'firstname'},{path:'lastname'}], formatter: '.formatter.workName' }" level="H2"/>-->
		<!--</semantic:titleHeading>-->
		<!--<semantic:headerContent>-->
		<semantic:content>
			<ObjectHeader intro="Organization ID: {external_id}" title="{path:'name'}" icon="sap-icon://employee-lookup"
				number="{ path: 'numberOfItems'}" numberUnit="" class="sapUiResponsiveMargin"
				numberState="{ path: 'last_status', formatter: '.formatter.statusType' }" titleActive="true" backgroundDesign="Translucent"
				condensed="false" visible="true">
				<attributes>
					<!--<ObjectAttribute text="{path:'email'}" active="false" visible="true"/>-->
					<ObjectAttribute text="{path:'corporate_name'}" active="false" visible="true"/>
					<ObjectAttribute text="{ path: 'last_timestamp', formatter: '.formatter.timestamp' }" active="false" visible="true"/>
				</attributes>
				<statuses>
					<ObjectStatus text="{ path: 'last_status', formatter: '.formatter.statusNumb' }"
						state="{ path: 'last_status', formatter: '.formatter.statusType' }"/>
				</statuses>
				<markers/>
			</ObjectHeader>
			<!--</semantic:headerContent>-->
			<!--<OverflowToolbar width="auto" height="46px" design="Solid" visible="true" enabled="true">-->
			<!--	<content>-->
