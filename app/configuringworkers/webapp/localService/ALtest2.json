{
	"global_parameters": {
		"ws_get_workers": {
			"request_filter": {
				"exclude_contingent_workers": "False",
				"exclude_employees": "False",
				"exclude_inactive_workers": "True"
			}
		}
	},
	"mappings": {
		"address_data": {
			"filters": {
				"filter": [{
					"business_site_address": "0",
					"address_format_type": "Extended",
					"_secuence": "1"
				}, {
					"business_site_address": "0",
					"_secuence": "2"
				}, {
					"business_site_address": "1",
					"_secuence": "3"
				}]
			},
			"country_logic": {
				"country": [{
					"address1": "{ADDRESS_LINE_1} - Number: {ADDRESS_LINE_3}",
					"address2": "{ADDRESS_LINE_2}",
					"address3": "{ADDRESS_LINE_4}",
					"_id": "Spain"
				}, {
					"address1": "{ADDRESS_LINE_1} - Number: {ADDRESS_LINE_3}",
					"address2": "{ADDRESS_LINE_2}",
					"address3": "{ADDRESS_LINE_4}",
					"_id": "Germany"
				}]
			}
		},
		"language": {
			"default_values": {
				"BPC7_default_code": "en-US"
			}
		},
		"zip_code": {
			"default_values": {
				"country": [{
					"_id": "ES",
					"__text": "28001"
				}, {
					"_id": "DE",
					"__text": "28002"
				}]
			}
		},
		"email": {
			"filters": {
				"filter": [{
					"usage_public": "1",
					"type_data_primary": "1",
					"type_reference": "WORK",
					"_secuence": "1"
				}, {
					"usage_public": "1",
					"type_data_primary": "1",
					"type_reference": "HOME",
					"_secuence": "2"
				}]
			}
		}
	},
	"transformations": {
		"organization_code": {
			"transformation": [{
				"characters": "/",
				"separator": ";",
				"replace_by": "_",
				"_secuence": "1"
			}, {
				"characters": "Ã‘;~",
				"separator": ";",
				"replace_by": "-",
				"_secuence": "2"
			}]
		}
	}
}