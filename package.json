{
  "name": "wd2pdsaas",
  "version": "0.0.1",
  "description": "Business Application",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/audit-logging": "^5",
    "@sap/hana-client": "^2",
    "@sap/cds-odata-v2-adapter-proxy": "^1",
    "@sap/xsenv": "^3",
    "@sap/xssec": "^3",
    "passport": "^0.4.1",
    "@sap/cds-mtx": "^2",
    "cfenv": "^1",
    "debug": "^4.3",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.90.0",
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "npx cds run"
  },
  "cds": {
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[production]": {
          "strategy": "JWT"
        },
        "[development]": {
          "strategy": "mock",
          "users": {
            "joe": {},
            "julie": {},
            "bob": {}
          }
        }
      },
      "db": {
        "[production]": {
          "kind": "hana"
        },
        "[development]": {
          "kind": "sql"
        },
        "model": [
          "db",
          "srv"
        ],
        "vcap": {
          "label": "service-manager"
        }
      },
      "multitenancy": true
    },
    "mtx": {
      "api": {
        "provisioning": true,
        "metadata": true,
        "model": true
      },
      "element-prefix": [
        "Z_",
        "ZZ_"
      ],
      "namespace-blocklist": [
        "com.sap.",
        "sap.",
        "wd2pdsaas."
      ],
      "extension-allowlist": [
        {
          "for": []
        },
        {
          "for": [
            "IntegrationService"
          ],
          "kind": "service"
        }
      ]
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "sapux": [
    "app/workerslist"
  ]
}
